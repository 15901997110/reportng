<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <title>$meta.reportTitle - $suite.name - Groups</title>
  <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
  <meta name="description" content="TestNG unit test results." />
  <link href="reportng.css" rel="stylesheet" type="text/css" />
</head>
<body>
  <div class="title" style="margin-bottom: 15px;">
    Groups for $suite.name
  </div>

  <table class="resultsTable" width="100%">
  #foreach ($group in $groups.keySet())
    <tr>
      <td class="group">$group</td>
    </tr>
    #foreach ($test in $groups.get($group))
    <tr>
      <td>${test.realClass.name}.<b>${test.methodName}</b></td>    
    </tr>
    #end
  #end
  </table>
</body>
</html>